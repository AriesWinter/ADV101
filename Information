// App.js

import React from 'react';
import {
  Button,
  SafeAreaView,
  Text,
  View,
} from 'react-native';

function clickMe() {
  alert ('Name: Jerry Mie M. Nalugon \nAge: 22 Years Old \nYear & Course: 3rd Year - BSIT')
}

const App = () => {
  return(
    <SafeAreaView>
      <View>
        <Text style={style}>My Information</Text>
      </View>

      <View style={buttonStyle}>
      <Button 
      color= '#000080'
      onPress={clickMe} 
      title='Press Me!'
      />
      </View>
    </SafeAreaView>
  );
}
const style = {
  fontSize: 25,
  textAlign: 'center',
  marginTop: 150
}
const buttonStyle = {
  width:"50%",
  marginTop: 20,
  marginLeft: 100
}




export default App;




// TERMINAL

